{
  "name": "cherry",
  "version": "1.0.0",
  "description": "React SSR framework",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.server.config.js --mode=production && webpack --config webpack.client.config.js --mode=production",
    "start": "node dist/server.js",
    "dev": "concurrently \"webpack --config webpack.server.config.js --mode=development --watch\" \"webpack --config webpack.client.config.js --mode=development --watch\" \"nodemon dist/server.js\""
  },
  "author": "Majid Ghafoorzade",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^8.0.1",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^5.3.4",
    "tsconfig-paths-webpack-plugin": "^4.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/react": "^18.2.5",
    "@types/react-dom": "^18.2.4",
    "@types/react-router-dom": "^5.3.3",
    "nodemon": "^2.0.22",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.0.2",
    "webpack-node-externals": "^3.0.0"
  }
}
